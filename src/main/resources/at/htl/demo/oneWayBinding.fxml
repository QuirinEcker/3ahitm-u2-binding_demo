<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 10;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.htl.demo.OneWayBindingController">
    <bottom>
        <AnchorPane prefHeight="30.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Previous" />
                <Button mnemonicParsing="false" text="Next" AnchorPane.rightAnchor="0.0" />
            </children>
        </AnchorPane>
    </bottom>
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" />
                <Button fx:id="showButton" mnemonicParsing="false" text="Button" />
                <Label fx:id="label" alignment="TOP_LEFT" text="Label" />
            </children>
        </VBox>
    </center>
</BorderPane>